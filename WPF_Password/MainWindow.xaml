<Window x:Class="WPF_Password.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Password"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Button Margin="0,0,300,200" Content="Password&#xD;&#xA;&#xD;&#xA;  &quot;Click&quot;" Height="90" Width="90" Click="Passwod_Click"></Button>
        <Button Margin="300,0,0,200" Content="Passwor Auto" Height="90" Width="90" Click="AutoPasswod_Click">



        </Button>

        <Button x:Name="btnSend" Content="Send"  Margin="0,0,600,0" Height="60" Width="90" Visibility="Collapsed" Click="Send_Click"></Button>




        <TextBox x:Name="SendTextBox" Margin="0,0,300,0"  Width="150" Height="40" Visibility="Collapsed"  BorderThickness="2"  ></TextBox>


        <TextBox  x:Name="PasswodTextBox" Margin="0,200,0,0"  Width="150" Height="40"   BorderThickness="2"  ></TextBox>

    </Grid>
</Window>
